s = int(input("Digite o valor a ser sacado: "))
while s > 0:
    s50 = s//50
    s = s - 50*s50
    s20 = s//20
    s = s - 20*s20
    s10 = s//10
    s = s - 10*s10
    s05 = s//5
    s = s - 5*s05
    s01 = s
    s = s - s
print(f"R$50,00: {s50}\n"
      f"R$20,00: {s20}\n"
      f"R$10,00: {s10}\n"
      f"R$5,00: {s05}\n"
      f"R$1,00: {s01}\n")
